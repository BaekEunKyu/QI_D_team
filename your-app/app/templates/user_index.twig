                        }

                        function initMap() { //초기화
                            // Initialize Google Maps
                            const mapOptions = {
                                center: new google.maps.LatLng(32.882632, -117.234817), //맵  처음 위치 
                                zoom: 15

                                /*1: World
                                5: Landmass/continent
                                10: City
                                15: Streets
                                20: Buildings*/
                            }
                            const map = new google.maps.Map(document.getElementById("map"), mapOptions); // 맵 만들기 

                            // Load JSON Data
                            const hotelMarkers = getJSONMarkers();

                            // Initialize Google Markers
                            for (hotel of hotelMarkers) {
                                let marker = new google.maps.Marker({
                                    map: map,
                                    position: new google.maps.LatLng(hotel.location[0], hotel.location[
                                        1]),
                                    title: hotel.name
                                })
                            }
                        }
